{
  "compileOnSave": false,
  "compilerOptions": {
      "rootDir": ".",
      "baseUrl": ".",
      "sourceMap": true,
      "allowJs": false,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "target": "es2015",
      "module": "esnext",
      "typeRoots": ["node_modules/@types"],
      "lib": ["es2017", "dom"],
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "resolveJsonModule": true,
      "paths": {}
  },
  "angularCompilerOptions": {
      "fullTemplateTypeCheck": true
  }
}
